{
    "compilerOptions": {
        "target": "ES2019",
        "module": "CommonJS",
        "lib": ["ES2019"],
        "moduleResolution": "node10",
        "baseUrl": "src",
        "outDir": "dist",
        "sourceMap": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
    },
    "include": ["src/**/*"],
    "exclude": [
        "src/**/*.test.ts",
        "node_modules",
        ".vscode"
    ]
}